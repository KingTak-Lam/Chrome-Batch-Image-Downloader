{
  "manifest_version": 3,
  "name": "批量图片下载器",
  "version": "1.0",
  "description": "选择并批量下载网页中的图片，支持跨域下载",
  "permissions": [
    "downloads",
    "activeTab",
    "scripting"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "action": {
    "default_popup": "popup.html"
  },
  "background": {
    "service_worker": "background.js"
  }
}
